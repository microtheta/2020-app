generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id   Int     @id @default(autoincrement())
  fid  String? @unique
  name String?
}

model migrations {
  id     Int      @id @default(autoincrement())
  name   String
  run_on DateTime
}

model migrations_state {
  key    String   @id
  value  String
  run_on DateTime
}
